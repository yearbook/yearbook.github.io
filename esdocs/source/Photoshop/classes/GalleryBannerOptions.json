{
  "name": "GalleryBannerOptions",
  "dynamic": true,
  "shortdesc": "Deprecated for Adobe Photoshop CS3, the make photo gallery method has moved to Bridge.",
  "elements": [
    {
      "type": "class",
      "property": [
        {
          "name": "siteName",
          "shortdesc": "Web photo gallery site name.",
          "datatype": {
            "type": "String",
            "value": "Adobe Web Photo Gallery"
          }
        },
        {
          "name": "photographer",
          "shortdesc": "Web photo gallery photographer.",
          "datatype": {
            "type": "String",
            "value": {}
          }
        },
        {
          "name": "contactInfo",
          "shortdesc": "Web photo gallery contact info.",
          "datatype": {
            "type": "String",
            "value": {}
          }
        },
        {
          "name": "date",
          "shortdesc": "Web photo gallery date.",
          "datatype": {
            "type": "String",
            "value": {}
          }
        },
        {
          "name": "font",
          "shortdesc": "The font setting for the banner text.",
          "datatype": {
            "type": "GalleryFontType",
            "value": "GalleryFontType.ARIAL"
          }
        },
        {
          "name": "fontSize",
          "shortdesc": "The size of the font for the banner text.",
          "datatype": {
            "type": "int",
            "min": 1,
            "max": 7,
            "value": 3
          }
        }
      ]
