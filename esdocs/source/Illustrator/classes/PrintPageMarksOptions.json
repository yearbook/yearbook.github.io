{
  "name": "PrintPageMarksOptions",
  "dynamic": true,
  "shortdesc": "The page marks options.",
  "elements": [
    {
      "type": "class",
      "property": [
        {
          "name": "pageMarksType",
          "shortdesc": "The page marks style.",
          "datatype": {
            "type": "PageMarksTypes",
            "value": "PageMarksTypes.Roman"
          }
        },
        {
          "name": "trimMarksWeight",
          "shortdesc": "Stroke weight of trim marks.",
          "datatype": {
            "type": "Number",
            "min": 0,
            "value": 0.125
          }
        },
        {
          "name": "trimMarks",
          "shortdesc": "Whether to enable trim marks printing.",
          "datatype": {
            "type": "Boolean",
            "value": false
          }
        },
        {
          "name": "registrationMarks",
          "shortdesc": "Whether to enable registration marks printing.",
          "datatype": {
            "type": "Boolean",
            "value": false
          }
        },
        {
          "name": "colorBars",
          "shortdesc": "Whether to enable color bars printing.",
          "datatype": {
            "type": "Boolean",
            "value": false
          }
        },
        {
          "name": "pageInfoMarks",
          "shortdesc": "Whether to enable page info marks printing.",
          "datatype": {
            "type": "Boolean",
            "value": false
          }
        },
        {
          "name": "bleedOffsetRect",
          "shortdesc": "The bleed offset rect.",
          "datatype": {
            "type": "Rect"
          }
        },
        {
          "name": "marksOffsetRect",
          "shortdesc": "The page marks offset rect.",
          "datatype": {
            "type": "Rect"
          }
        }
      ]
    }
  ]
}