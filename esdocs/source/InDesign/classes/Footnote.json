{
  "name": "Footnote",
  "dynamic": true,
  "shortdesc": "A footnote.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "storyOffset",
          "rwaccess": "readonly",
          "shortdesc": "The location of the footnote marker in the parent story.",
          "datatype": {
            "type": "InsertionPoint"
          }
        },
        {
          "name": "allPageItems",
          "rwaccess": "readonly",
          "shortdesc": "Lists all page items contained by the Footnote.",
          "datatype": {
            "type": "PageItem",
            "array": true
          }
        },
        {
          "name": "allGraphics",
          "rwaccess": "readonly",
          "shortdesc": "Lists all graphics contained by the Footnote.",
          "datatype": {
            "type": "Graphic",
            "array": true
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the Footnote.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Footnote (a XmlStory, Story, TextFrame or InsertionPoint).",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Footnote within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "textColumns",
          "rwaccess": "readonly",
          "shortdesc": "A collection of text columns.",
          "datatype": {
            "type": "TextColumns"
          }
        },
        {
          "name": "texts",
          "rwaccess": "readonly",
          "shortdesc": "A collection of text objects.",
          "datatype": {
            "type": "Texts"
          }
        },
        {
          "name": "textStyleRanges",
          "rwaccess": "readonly",
          "shortdesc": "A collection of text style ranges.",
          "datatype": {
            "type": "TextStyleRanges"
          }
        },
        {
          "name": "paragraphs",
          "rwaccess": "readonly",
          "shortdesc": "A collection of paragraphs.",
          "datatype": {
            "type": "Paragraphs"
          }
        },
        {
          "name": "lines",
          "rwaccess": "readonly",
          "shortdesc": "A collection of lines.",
          "datatype": {
            "type": "Lines"
          }
        },
        {
          "name": "words",
          "rwaccess": "readonly",
          "shortdesc": "A collection of words.",
          "datatype": {
            "type": "Words"
          }
        },
        {
          "name": "characters",
          "rwaccess": "readonly",
          "shortdesc": "A collection of characters.",
          "datatype": {
            "type": "Characters"
          }
        },
        {
          "name": "insertionPoints",
          "rwaccess": "readonly",
          "shortdesc": "A collection of insertion points.",
          "datatype": {
            "type": "InsertionPoints"
          }
        },
        {
          "name": "textVariableInstances",
          "rwaccess": "readonly",
          "shortdesc": "A collection of text variable instances.",
          "datatype": {
            "type": "TextVariableInstances"
          }
        },
        {
          "name": "ovals",
          "rwaccess": "readonly",
          "shortdesc": "A collection of ellipses.",
          "datatype": {
            "type": "Ovals"
          }
        },
        {
          "name": "splineItems",
          "rwaccess": "readonly",
          "shortdesc": "The spline items collection.",
          "datatype": {
            "type": "SplineItems"
          }
        },
        {
          "name": "pageItems",
          "rwaccess": "readonly",
          "shortdesc": "The page items collection, which can be used to process all page items in a container (such as a document, page, or group), regardless of type.",
          "datatype": {
            "type": "PageItems"
          }
        },
        {
          "name": "rectangles",
          "rwaccess": "readonly",
          "shortdesc": "A collection of rectangles.",
          "datatype": {
            "type": "Rectangles"
          }
        },
        {
          "name": "graphicLines",
          "rwaccess": "readonly",
          "shortdesc": "A collection of graphic lines.",
          "datatype": {
            "type": "GraphicLines"
          }
        },
        {
          "name": "textFrames",
          "rwaccess": "readonly",
          "shortdesc": "A collection of text frames.",
          "datatype": {
            "type": "TextFrames"
          }
        },
        {
          "name": "polygons",
          "rwaccess": "readonly",
          "shortdesc": "A collection of polygons.",
          "datatype": {
            "type": "Polygons"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "groups",
          "rwaccess": "readonly",
          "shortdesc": "A collection of groups.",
          "datatype": {
            "type": "Groups"
          }
        },
        {
          "name": "epstexts",
          "rwaccess": "readonly",
          "shortdesc": "EPSTexts",
          "datatype": {
            "type": "EPSTexts"
          }
        },
        {
          "name": "hiddenTexts",
          "rwaccess": "readonly",
          "shortdesc": "A collection of hidden text objects.",
          "datatype": {
            "type": "HiddenTexts"
          }
        },
        {
          "name": "contents",
          "shortdesc": "The text contents of the footnote. Can return: String, SpecialCharacters enumerator or Array of Strings or SpecialCharacters enumerators. Can also accept: NothingEnum enumerator or Array of Strings, SpecialCharacters enumerators or NothingEnum enumerators.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "label",
          "shortdesc": "A property that can be set to any string.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "name",
          "shortdesc": "The name of the Footnote; this is an alias to the Footnote's label property.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the Footnote."
        },
        {
          "name": "convertToText",
          "shortdesc": "Converts the footnote to part of the story text and places the converted text at the former location of the footnote marker in the text.",
          "datatype": {
            "type": "Text"
          }
        },
        {
          "name": "insertLabel",
          "shortdesc": "Sets the label to the value associated with the specified key.",
          "parameters": [
            {
              "name": "key",
              "shortdesc": "The key.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "value",
              "shortdesc": "The value.",
              "datatype": {
                "type": "String"
              }
            }
          ]
        },
        {
          "name": "extractLabel",
          "shortdesc": "Gets the label value associated with the specified key.",
          "parameters": [
            {
              "name": "key",
              "shortdesc": "The key.",
              "datatype": {
                "type": "String"
              }
            }
          ],
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the Footnote.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "Footnote",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The registered event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "Boolean"
          }
        }
      ]
    }
  ]
}