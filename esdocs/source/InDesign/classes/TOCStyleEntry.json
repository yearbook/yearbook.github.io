{
  "name": "TOCStyleEntry",
  "dynamic": true,
  "shortdesc": "A TOC entry definition.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the TOCStyleEntry (a TOCStyle).",
          "datatype": {
            "type": "TOCStyle"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the TOCStyleEntry within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "name",
          "shortdesc": "The name of the TOCStyleEntry.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "formatStyle",
          "shortdesc": "The paragraph style applied to the TOC entry. Can return: ParagraphStyle or String.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "level",
          "shortdesc": "The indent level of the entry in the TOC.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "pageNumberStyle",
          "shortdesc": "The character style applied to the page number of the entry. Can return: CharacterStyle or String.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "pageNumberPosition",
          "shortdesc": "The page number placement for the TOC entry style.",
          "datatype": {
            "type": "PageNumberPosition"
          }
        },
        {
          "name": "separator",
          "shortdesc": "The string to insert between the entry text and the page numbers.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "separatorStyle",
          "shortdesc": "The character style applied to the string separating the TOC entry text and the page numbers. Can return: CharacterStyle or String.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "sortAlphabet",
          "shortdesc": "If true, sorts the entries alphabetically.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the TOCStyleEntry."
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the TOCStyleEntry.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "TOCStyleEntry",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The registered event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "Boolean"
          }
        }
      ]
    }
  ]
}