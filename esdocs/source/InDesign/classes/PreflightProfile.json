{
  "name": "PreflightProfile",
  "dynamic": true,
  "shortdesc": "A preflight profile.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the PreflightProfile.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the PreflightProfile (a Application or Document).",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the PreflightProfile within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "preflightProfileRules",
          "rwaccess": "readonly",
          "shortdesc": "A collection of preflight profile rules.",
          "datatype": {
            "type": "PreflightProfileRules"
          }
        },
        {
          "name": "preflightRuleInstances",
          "rwaccess": "readonly",
          "shortdesc": "A collection of preflight rule instances.",
          "datatype": {
            "type": "PreflightRuleInstances"
          }
        },
        {
          "name": "name",
          "shortdesc": "The name of the PreflightProfile.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "description",
          "shortdesc": "The description of the PreflightProfile.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "label",
          "shortdesc": "A property that can be set to any string.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the PreflightProfile."
        },
        {
          "name": "duplicate",
          "shortdesc": "Duplicates the PreflightProfile.",
          "datatype": {
            "type": "PreflightProfile"
          }
        },
        {
          "name": "update",
          "shortdesc": "Update the preflight profile by copying from another profile.",
          "parameters": [
            {
              "name": "using",
              "shortdesc": "The preflight profile to copy. Can accept: String or PreflightProfile. ",
              "datatype": {
                "type": "Mixed"
              },
              "optional": true
            }
          ]
        },
        {
          "name": "unembed",
          "shortdesc": "Unembed this profile."
        },
        {
          "name": "save",
          "shortdesc": "Saves this preflight profile to a InDesign preflight profile file.",
          "parameters": [
            {
              "name": "to",
              "shortdesc": "The preflight profile file to save to.",
              "datatype": {
                "type": "File"
              }
            }
          ]
        },
        {
          "name": "insertLabel",
          "shortdesc": "Sets the label to the value associated with the specified key.",
          "parameters": [
            {
              "name": "key",
              "shortdesc": "The key.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "value",
              "shortdesc": "The value.",
              "datatype": {
                "type": "String"
              }
            }
          ]
        },
        {
          "name": "extractLabel",
          "shortdesc": "Gets the label value associated with the specified key.",
          "parameters": [
            {
              "name": "key",
              "shortdesc": "The key.",
              "datatype": {
                "type": "String"
              }
            }
          ],
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the PreflightProfile.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "PreflightProfile",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
