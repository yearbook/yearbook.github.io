{
  "name": "MarginPreference",
  "dynamic": true,
  "superclass": {
    "href": "#/Preference",
    "$t": "Preference"
  },
  "shortdesc": "Margin preferences.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "customColumns",
          "rwaccess": "readonly",
          "shortdesc": "If false, columns are evenly spaced. If true, columns can have custom widths.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the MarginPreference (a Application, Document or Page).",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "columnCount",
          "shortdesc": "The number of columns to place on the page.",
          "datatype": {
            "type": "Number",
            "min": 1,
            "max": 216
          }
        },
        {
          "name": "columnGutter",
          "shortdesc": "The distance between columns.",
          "datatype": {
            "type": "Mixed",
            "min": 0,
            "max": 1440
          }
        },
        {
          "name": "top",
          "shortdesc": "The top edge of the MarginPreference.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "bottom",
          "shortdesc": "The bottom edge of the MarginPreference.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "left",
          "shortdesc": "The left edge of the MarginPreference.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "right",
          "shortdesc": "The right edge of the MarginPreference.",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "columnDirection",
          "shortdesc": "The direction of text in the column.",
          "datatype": {
            "type": "HorizontalOrVertical"
          }
        },
        {
          "name": "columnsPositions",
          "shortdesc": "The distance that each column guide is placed from the left margin, formatted as an array in the format [guide1, guide2, guide3].",
          "datatype": {
            "type": "Mixed",
            "array": true
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the MarginPreference.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "MarginPreference",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The registered event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. ",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "Boolean"
          }
        }
      ]
    }
  ]
}