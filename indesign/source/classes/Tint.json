{
  "name": "Tint",
  "dynamic": true,
  "superclass": {
    "href": "#/Color",
    "$t": "Color"
  },
  "shortdesc": "A tint swatch.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "baseColor",
          "rwaccess": "readonly",
          "shortdesc": "The color that the tint is based on.",
          "datatype": {
            "type": "Color"
          }
        },
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The name of the Tint.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "model",
          "rwaccess": "readonly",
          "shortdesc": "The color model.",
          "datatype": {
            "type": "ColorModel"
          }
        },
        {
          "name": "space",
          "rwaccess": "readonly",
          "shortdesc": "The color space.",
          "datatype": {
            "type": "ColorSpace"
          }
        },
        {
          "name": "colorValue",
          "rwaccess": "readonly",
          "shortdesc": "The ink values that create the color, specified as a percentage for each ink. Note: The number of values required and the range depends on the color space. For RGB, specify three values, with each value in the range 0 to 255; for CMYK, specify four values representing C, M, Y, and K, with each value in the range 0 to 100; for LAB, specify three values representing L &amp;&#35;40;Range: 0 to 100&#41;, A (Range: -128 to 127), and B (Range: -128 to 127); for mixed ink, specify values for each ink in the ink list, with each value in the range 0 to 100.",
          "datatype": {
            "type": "number",
            "array": true
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the Tint.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Tint &amp;&#35;40;a Application or Document&#41;.",
          "datatype": {
            "type": "mixed"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Tint within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "tintValue",
          "shortdesc": "The percent of the base color. &amp;&#35;40;Range: 0 to 100&#41;",
          "datatype": {
            "type": "number",
            "min": 0,
            "max": 100
          }
        },
        {
          "name": "label",
          "shortdesc": "A property that can be set to any string.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the swatch.",
          "parameters": {
            "parameter": [
              {
                "name": "replacingWith",
                "shortdesc": "The swatch to apply in place of the deleted swatch. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "Swatch"
                }
              }
            ]
          }
        },
        {
          "name": "duplicate",
          "shortdesc": "Duplicates the Tint.",
          "datatype": {
            "type": "Tint"
          }
        },
        {
          "name": "merge",
          "shortdesc": "Merges the specified swatches with the swatch.",
          "parameters": {
            "parameter": [
              {
                "name": "with",
                "shortdesc": "The swatches to merge. Can accept: Array of Swatches, Swatch or String.",
                "datatype": {
                  "type": "mixed"
                }
              }
            ]
          },
          "datatype": {
            "type": "Swatch"
          }
        },
        {
          "name": "insertLabel",
          "shortdesc": "Sets the label to the value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "value",
                "shortdesc": "The value.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          }
        },
        {
          "name": "extractLabel",
          "shortdesc": "Gets the label value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          },
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the Tint.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "Tint",
            "array": {}
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The registered event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "bool"
          }
        }
      ]
    }
  ]
}