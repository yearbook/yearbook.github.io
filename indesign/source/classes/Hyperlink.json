{
  "name": "Hyperlink",
  "dynamic": true,
  "shortdesc": "A hyperlink.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "hidden",
          "rwaccess": "readonly",
          "shortdesc": "If true, the hyperlink is hidden.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the Hyperlink.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Hyperlink &amp;&#35;40;a Document&#41;.",
          "datatype": {
            "type": "Document"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Hyperlink within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "name",
          "shortdesc": "The name of the Hyperlink.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "source",
          "shortdesc": "The hyperlinked text or page item. Can return: HyperlinkPageItemSource, HyperlinkTextSource or CrossReferenceSource.",
          "datatype": {
            "type": "mixed"
          }
        },
        {
          "name": "visible",
          "shortdesc": "If true, the Hyperlink is visible.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "highlight",
          "shortdesc": "The hyperlink highlight style.",
          "datatype": {
            "type": "HyperlinkAppearanceHighlight"
          }
        },
        {
          "name": "width",
