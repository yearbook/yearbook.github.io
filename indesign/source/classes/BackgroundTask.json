{
  "name": "BackgroundTask",
  "dynamic": true,
  "shortdesc": "background task",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The name of the BackgroundTask.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "documentName",
          "rwaccess": "readonly",
          "shortdesc": "The document name on which this task operates.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "percentDone",
          "rwaccess": "readonly",
          "shortdesc": "Progress information for this task.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "status",
          "rwaccess": "readonly",
          "shortdesc": "The current status of this task object.",
          "datatype": {
            "type": "TaskState"
          }
        },
        {
          "name": "alerts",
          "rwaccess": "readonly",
          "shortdesc": "The alerts encountered while running this task object. Can return: Array of Array of 2 TaskAlertType enumerators or Strings.",
          "datatype": {
            "type": "Mixed",
            "array": true
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the BackgroundTask.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the BackgroundTask &#40;a Application).",
          "datatype": {
            "type": "Application"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the BackgroundTask within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "cancelTask",
          "shortdesc": "Cancels the specified background task."
        },
        {
          "name": "waitForTask",
          "shortdesc": "Waits for the task to finish.",
          "datatype": {
            "type": "TaskState"
          }
        },
        {
          "name": "queryProperty",
          "shortdesc": "Queries for a particular property in the task metadata.",
          "parameters": [
            {
              "name": "name",
              "shortdesc": "The task property being queried",
              "datatype": {
                "type": "String"
              }
            }
          ],
          "datatype": {
            "type": "any"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the BackgroundTask.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "BackgroundTask",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. &#40;Optional)",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The registered event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. &#40;Optional)",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "Boolean"
          }
        }
      ]
    }
  ]
}