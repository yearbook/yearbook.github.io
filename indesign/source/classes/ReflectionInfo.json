{
  "name": "ReflectionInfo",
  "dynamic": true,
  "shortdesc": "Provides information about a method, a property or a method parameters.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The class object that this element belongs to.",
          "datatype": {
            "type": "Reflection"
          }
        },
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The element name.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "type",
          "rwaccess": "readonly",
          "shortdesc": "The element type.",
          "description": "One of unknown, readonly, readwrite, createonly, method or parameter.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "dataType",
          "rwaccess": "readonly",
          "shortdesc": "The data type.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "arguments",
          "rwaccess": "readonly",
          "shortdesc": "The description of method or function arguments.",
          "datatype": {
            "type": "ReflectionInfo",
            "array": true
          }
        },
        {
          "name": "min",
          "rwaccess": "readonly",
          "shortdesc": "The minimum value.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "max",
          "rwaccess": "readonly",
          "shortdesc": "The maximum value.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "defaultValue",
          "rwaccess": "readonly",
          "shortdesc": "The default value.",
          "datatype": {
            "type": "any"
          }
        },
        {
          "name": "isCollection",
          "rwaccess": "readonly",
          "shortdesc": "Contains true if the class describes a collection class.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "description",
          "rwaccess": "readonly",
          "shortdesc": "The long description text.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "help",
          "rwaccess": "readonly",
          "shortdesc": "The short description text.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "sampleCode",
          "rwaccess": "readonly",
          "shortdesc": "Sample code, if present.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "sampleFile",
          "rwaccess": "readonly",
          "shortdesc": "A file containing sample code. May be null.",
          "datatype": {
            "type": "File"
          }
        }
      ]
    }
  ]
}