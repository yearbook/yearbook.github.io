{
  "name": "Library",
  "dynamic": true,
  "shortdesc": "An object library.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The name of the Library.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "fullName",
          "rwaccess": "readonly",
          "shortdesc": "The full path to the Library, including the name of the Library.",
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "filePath",
          "rwaccess": "readonly",
          "shortdesc": "The full path to the file.",
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "associatedPanel",
          "rwaccess": "readonly",
          "shortdesc": "The associated panel.",
          "datatype": {
            "type": "Panel"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Library &amp;&#35;40;a Application&#41;.",
          "datatype": {
            "type": "Application"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Library within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "assets",
          "rwaccess": "readonly",
          "shortdesc": "A collection of object library assets.",
          "datatype": {
            "type": "Assets"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "close",
          "shortdesc": "Closes the Library."
        },
        {
          "name": "store",
          "shortdesc": "Stores the specified object in the library.",
          "parameters": {
            "parameter": [
              {
                "name": "using",
                "shortdesc": "The page item&amp;&#35;40;s&#41; to store. Can accept: Array of PageItems, Movies, Sounds, Graphics or XMLElements.",
                "datatype": {
                  "type": "mixed",
                  "array": {}
                }
              },
              {
                "name": "withProperties",
                "shortdesc": "Initial values for properties of the new Library &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "Object"
                }
              }
            ]
          },
          "datatype": {
            "type": "Asset"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the Library.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "Library",
            "array": {}
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The registered event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "bool"
          }
        }
      ]
    }
  ]
}