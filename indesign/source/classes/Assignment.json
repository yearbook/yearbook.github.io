{
  "name": "Assignment",
  "dynamic": true,
  "shortdesc": "An assignment.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "documentPath",
          "rwaccess": "readonly",
          "shortdesc": "The path to the document that the hyperlink destination points to.",
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "assignmentFileStatus",
          "rwaccess": "readonly",
          "shortdesc": "The status of the assignment file.",
          "datatype": {
            "type": "AssignmentStatus"
          }
        },
        {
          "name": "packaged",
          "rwaccess": "readonly",
          "shortdesc": "If true, the assignment is packaged.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "packageUpToDate",
          "rwaccess": "readonly",
          "shortdesc": "If true, the assignment package is up to date.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "filePath",
          "rwaccess": "readonly",
          "shortdesc": "The file path &amp;&#35;40;colon delimited on the Mac OS&#41;. Can also accept: File.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the Assignment.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Assignment &amp;&#35;40;a Document&#41;.",
          "datatype": {
            "type": "Document"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Assignment within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "assignedStories",
          "rwaccess": "readonly",
          "shortdesc": "A collection of assigned stories.",
          "datatype": {
            "type": "AssignedStories"
          }
        },
        {
          "name": "name",
          "shortdesc": "The name of the Assignment.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "userName",
          "shortdesc": "The user name to assign to tracked changes and notes.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "exportOptions",
          "shortdesc": "The content to export in the assignment.",
          "datatype": {
            "type": "AssignmentExportOptions"
          }
        },
        {
          "name": "frameColor",
          "shortdesc": "The color of the assignment&apos;s frames. Can return: Array of 3 Reals &amp;&#35;40;0 - 255&#41; or UIColors enumerator or NothingEnum enumerator.",
          "datatype": {
            "type": "mixed"
          }
        },
        {
          "name": "includeLinksWhenPackage",
          "shortdesc": "If true, includes linked files when packaging the assignment.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "label",
          "shortdesc": "A property that can be set to any string.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "update",
          "shortdesc": "Updates the assignment file.",
          "parameters": {
            "parameter": [
              {
                "name": "versionComments",
                "shortdesc": "The comment for this version. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "forceSave",
                "optional": true,
                "shortdesc": "If true, forcibly saves a version. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          }
        },
        {
          "name": "remove",
          "shortdesc": "Deletes the assignment and its file."
        },
        {
          "name": "createPackage",
          "shortdesc": "Creates an assignment package.",
          "parameters": {
            "parameter": [
              {
                "name": "filePath",
                "shortdesc": "The full path name of the assignment package file.",
                "datatype": {
                  "type": "File"
                }
              },
              {
                "name": "submit",
                "optional": true,
                "shortdesc": "If true, submits assigned stories before packaging the assignment. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": true
                }
              },
              {
                "name": "withProperties",
                "shortdesc": "Initial values for properties of the new Assignment &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "Object"
                }
              }
            ]
          },
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "cancelPackage",
          "shortdesc": "Cancels the package for an assignment."
        },
        {
          "name": "insertLabel",
          "shortdesc": "Sets the label to the value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "value",
                "shortdesc": "The value.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          }
        },
        {
          "name": "extractLabel",
          "shortdesc": "Gets the label value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          },
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the Assignment.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "Assignment",
            "array": {}
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The registered event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "bool"
          }
        }
      ]
    }
  ]
}