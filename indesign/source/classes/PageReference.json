          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the PageReference &amp;&#35;40;a Topic&#41;.",
          "datatype": {
            "type": "Topic"
          }
        },
        {
          "name": "index",
