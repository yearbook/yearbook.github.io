{
  "name": "Submenu",
  "dynamic": true,
  "superclass": {
    "href": "#/MenuElement",
    "$t": "MenuElement"
  },
  "shortdesc": "A submenu.",
  "elements": [
    {
      "type": "class",
      "property": [
        {
          "name": "BEFORE_DISPLAY",
          "rwaccess": "readonly",
          "shortdesc": "Dispatched before the Submenu is displayed. This event does not bubble. This event is not cancelable.",
          "datatype": {
            "type": "String",
            "value": "beforeDisplay"
          }
        }
      ]
    },
    {
      "type": "instance",
      "property": [
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The name of the Submenu.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "title",
          "rwaccess": "readonly",
          "shortdesc": "The name of the Submenu for display in the user interface. The title includes any ampersand characters &#40;&&#41;, which are used to tell the Windows OS to underline the following character in the name for use with the Alt key to navigate to a menu item. Double ampersands are used to display an actual ampersand character in the name. The Mac OS ignores and removes the extra ampersand characters.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the Submenu &#40;a Menu or Submenu).",
          "datatype": {
            "type": "Mixed"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the Submenu within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "menuElements",
          "rwaccess": "readonly",
          "shortdesc": "A collection of menu elements.",
          "datatype": {
            "type": "MenuElements"
          }
        },
        {
          "name": "submenus",
          "rwaccess": "readonly",
          "shortdesc": "A collection of submenus.",
          "datatype": {
            "type": "Submenus"
          }
        },
        {
          "name": "menuItems",
          "rwaccess": "readonly",
          "shortdesc": "A collection of menu items.",
          "datatype": {
            "type": "MenuItems"
          }
        },
        {
          "name": "menuSeparators",
          "rwaccess": "readonly",
          "shortdesc": "A collection of menu separators.",
          "datatype": {
            "type": "MenuSeparators"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the Submenu."
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the Submenu.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "Submenu",
            "array": true
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "String"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. &#40;Optional)",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": [
            {
              "name": "eventType",
              "shortdesc": "The registered event type.",
              "datatype": {
                "type": "String"
              }
            },
            {
              "name": "handler",
              "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
              "datatype": {
                "type": "Mixed"
              }
            },
            {
              "name": "captures",
              "optional": true,
              "shortdesc": "This parameter is obsolete. &#40;Optional)",
              "datatype": {
                "type": "Boolean",
                "value": false
              }
            }
          ],
          "datatype": {
            "type": "Boolean"
          }
        }
      ]
    }
  ]
}