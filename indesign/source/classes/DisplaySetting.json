{
  "name": "DisplaySetting",
  "dynamic": true,
  "shortdesc": "Object-level display settings.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the DisplaySetting &#40;a Application).",
          "datatype": {
            "type": "Application"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the DisplaySetting within its containing object.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "raster",
          "shortdesc": "The display method for raster images.",
          "datatype": {
            "type": "TagRaster"
          }
        },
        {
          "name": "vector",
          "shortdesc": "The display method for vector images.",
          "datatype": {
            "type": "TagVector"
          }
        },
        {
