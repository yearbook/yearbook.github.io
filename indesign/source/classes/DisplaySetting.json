          "shortdesc": "The display setting for transparencies.",
          "datatype": {
            "type": "TagTransparency"
          }
        },
        {
          "name": "antialiasing",
          "shortdesc": "If true, uses anti-aliasing for text and bitmap images.",
          "datatype": {
            "type": "Boolean"
          }
        },
        {
          "name": "greekBelow",
          "shortdesc": "The point size below which text is greeked.",
          "datatype": {
            "type": "Number"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
