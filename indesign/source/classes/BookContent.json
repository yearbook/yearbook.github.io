{
  "name": "BookContent",
  "dynamic": true,
  "shortdesc": "A document added to a book.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "fullName",
          "rwaccess": "readonly",
          "shortdesc": "The full path to the BookContent, including the name of the BookContent.",
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "status",
          "rwaccess": "readonly",
          "shortdesc": "The status of the book content object file.",
          "datatype": {
            "type": "BookContentStatus"
          }
        },
        {
          "name": "size",
          "rwaccess": "readonly",
          "shortdesc": "The size of the BookContent file.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "date",
          "rwaccess": "readonly",
          "shortdesc": "The date and time the BookContent was created.",
          "datatype": {
            "type": "Date"
          }
        },
        {
          "name": "documentPageRange",
          "rwaccess": "readonly",
          "shortdesc": "The page range of the book content object within the book.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "filePath",
          "rwaccess": "readonly",
          "shortdesc": "The full path to the file.",
          "datatype": {
            "type": "File"
          }
        },
        {
          "name": "name",
          "rwaccess": "readonly",
          "shortdesc": "The name of the BookContent.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the BookContent.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the BookContent &amp;&#35;40;a Book&#41;.",
          "datatype": {
            "type": "Book"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the BookContent within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "label",
          "shortdesc": "A property that can be set to any string.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "move",
          "shortdesc": "Moves the book content object.",
          "parameters": {
            "parameter": [
              {
                "name": "to",
                "optional": true,
                "shortdesc": "The location relative to the reference object or within the book. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "LocationOptions",
                  "value": "LocationOptions.AT_END"
                }
              },
              {
                "name": "reference",
                "shortdesc": "The reference object. Note: Required when the to parameter specifies before or after. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "BookContent"
                }
              }
            ]
          },
          "datatype": {
            "type": "BookContent"
          }
        },
        {
          "name": "remove",
          "shortdesc": "Deletes the BookContent."
        },
        {
          "name": "replace",
          "shortdesc": "Replaces a book content object with a new file. Note: If the new file replaces the current style source document, the new file becomes the style source document.",
          "parameters": {
            "parameter": [
              {
                "name": "using",
                "shortdesc": "The full path name of the new book content object.",
                "datatype": {
                  "type": "File"
                }
              }
            ]
          },
          "datatype": {
            "type": "BookContent"
          }
        },
        {
          "name": "synchronize",
          "shortdesc": "Matches the formatting of the book content object to the style source document."
        },
        {
          "name": "preflight",
          "shortdesc": "Preflight a book content object and optionally save the resulting report.",
          "parameters": {
            "parameter": [
              {
                "name": "to",
                "shortdesc": "The preflight report to save to. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "File"
                }
              },
              {
                "name": "autoOpen",
                "optional": true,
                "shortdesc": "If true, automatically open the report after creation. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          }
        },
        {
          "name": "insertLabel",
          "shortdesc": "Sets the label to the value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "value",
                "shortdesc": "The value.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          }
        },
        {
          "name": "extractLabel",
          "shortdesc": "Gets the label value associated with the specified key.",
          "parameters": {
            "parameter": [
              {
                "name": "key",
                "shortdesc": "The key.",
                "datatype": {
                  "type": "string"
                }
              }
            ]
          },
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the BookContent.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "BookContent",
            "array": {}
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The registered event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "bool"
          }
        }
      ]
    }
  ]
}