{
  "name": "DTD",
  "dynamic": true,
  "superclass": {
    "href": "#/XMLItem",
    "$t": "XMLItem"
  },
  "shortdesc": "The document DTD.",
  "elements": [
    {
      "type": "instance",
      "property": [
        {
          "name": "systemId",
          "rwaccess": "readonly",
          "shortdesc": "The system ID of the DOCTYPE declaration. Note: Valid only when the DTD is an external subset.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "publicId",
          "rwaccess": "readonly",
          "shortdesc": "The public ID of the DOCTYPE declaration. Note: Valid only when the DTD is an external subset.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "contents",
          "rwaccess": "readonly",
          "shortdesc": "The contents of the text. Can return: String or SpecialCharacters enumerator.",
          "datatype": {
            "type": "mixed"
          }
        },
        {
          "name": "id",
          "rwaccess": "readonly",
          "shortdesc": "The unique ID of the DTD.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "isValid",
          "rwaccess": "readonly",
          "shortdesc": "Returns true if the object specifier resolves to valid objects.",
          "datatype": {
            "type": "bool"
          }
        },
        {
          "name": "parent",
          "rwaccess": "readonly",
          "shortdesc": "The parent of the DTD &amp;&#35;40;a Document&#41;.",
          "datatype": {
            "type": "Document"
          }
        },
        {
          "name": "index",
          "rwaccess": "readonly",
          "shortdesc": "The index of the DTD within its containing object.",
          "datatype": {
            "type": "number"
          }
        },
        {
          "name": "events",
          "rwaccess": "readonly",
          "shortdesc": "A collection of events.",
          "datatype": {
            "type": "Events"
          }
        },
        {
          "name": "eventListeners",
          "rwaccess": "readonly",
          "shortdesc": "A collection of event listeners.",
          "datatype": {
            "type": "EventListeners"
          }
        },
        {
          "name": "rootTag",
          "shortdesc": "The tag of the root object.",
          "datatype": {
            "type": "XMLTag"
          }
        },
        {
          "name": "properties",
          "shortdesc": "A property that allows setting of several properties at the same time.",
          "datatype": {
            "type": "Object"
          }
        }
      ],
      "method": [
        {
          "name": "remove",
          "shortdesc": "Deletes the DTD."
        },
        {
          "name": "select",
          "shortdesc": "Selects the object.",
          "parameters": {
            "parameter": [
              {
                "name": "existingSelection",
                "optional": true,
                "shortdesc": "The selection status of the DTD in relation to previously selected objects. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "SelectionOptions",
                  "value": "SelectionOptions.REPLACE_WITH"
                }
              }
            ]
          }
        },
        {
          "name": "toSource",
          "shortdesc": "Generates a string which, if executed, will return the DTD.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "getElements",
          "shortdesc": "Resolves the object specifier, creating an array of object references.",
          "datatype": {
            "type": "DTD",
            "array": {}
          }
        },
        {
          "name": "toSpecifier",
          "shortdesc": "Retrieves the object specifier.",
          "datatype": {
            "type": "string"
          }
        },
        {
          "name": "addEventListener",
          "shortdesc": "Adds an event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "EventListener"
          }
        },
        {
          "name": "removeEventListener",
          "shortdesc": "Removes the event listener.",
          "parameters": {
            "parameter": [
              {
                "name": "eventType",
                "shortdesc": "The registered event type.",
                "datatype": {
                  "type": "string"
                }
              },
              {
                "name": "handler",
                "shortdesc": "The registered event handler. Can accept: File or JavaScript Function.",
                "datatype": {
                  "type": "mixed"
                }
              },
              {
                "name": "captures",
                "optional": true,
                "shortdesc": "This parameter is obsolete. &amp;&#35;40;Optional&#41;",
                "datatype": {
                  "type": "bool",
                  "value": false
                }
              }
            ]
          },
          "datatype": {
            "type": "bool"
          }
        }
      ]
    }
  ]
}